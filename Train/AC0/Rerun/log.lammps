variable        e1 equal f_2[2]
variable        e2 equal f_2[3]
variable        vir0 equal f_2[4]
variable        vir1 equal f_2[5]
variable        vir2 equal f_2[6]
variable        vir3 equal f_2[7]
variable        vir4 equal f_2[8]
variable        vir5 equal f_2[9]

